{
  "private": false,
  "name": "gutenberg-starter-plugin",
  "version": "0.1.0",
  "author": "Michael Kantor",
  "license": "MIT",
  "browserslist": [
    "last 2 firefox versions",
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 safari versions",
    "ie >= 11",
    "not ie < 10",
    ">1%"
  ],
  "scripts": {
    "build": "webpack --config=.config/webpack.config.prod.js",
    "start": "webpack-dev-server --config=.config/webpack.config.dev.js",
    "lint:js": "eslint --cache-location=.cache/eslint.json --ignore-path=.config/.eslintignore --ext js,jsx,ts,tsx .eslintrc.js .config src blocks",
    "lint:js:fix": "eslint --fix --cache-location=.cache/eslint.json --ignore-path=.config/.eslintignore --ext js,jsx,ts,tsx .eslintrc.js .config src blocks",
    "lint": "npm-run-all lint:*",
    "test:php": "make test-php",
    "test:js": " jest --env=jest-environment-jsdom-sixteen --runInBand",
    "test:js:watch": " jest --watch --env=jest-environment-jsdom-sixteen --runInBand",
    "test:js:coverage": " jest --coverage --env=jest-environment-jsdom-sixteen --runInBand",
    "test:js:snapshots": " jest --updateSnapshot --env=jest-environment-jsdom-sixteen --runInBand",
    "test": "npm-run-all test:*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@humanmade/webpack-helpers": "0.9.0",
    "@testing-library/dom": "^7.24.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@wordpress/babel-preset-default": "^4.18.1",
    "@wordpress/blocks": "^6.22.0",
    "@wordpress/components": "^10.2.0",
    "@wordpress/data": "^4.23.1",
    "@wordpress/edit-post": "^3.23.0",
    "@wordpress/editor": "^9.22.0",
    "@wordpress/jest-preset-default": "^6.3.0",
    "@wordpress/server-side-render": "^1.17.0",
    "@wordpress/url": "^2.18.0",
    "babel-eslint": "^10.1.0",
    "block-editor-hmr": "0.6.3",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "jest-css-modules-transform": "^4.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-extended": "^0.11.5",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.11",
    "ts-jest": "^26.4.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/css-mediaquery": "^0.1.0",
    "@types/jest": "^25.2.2",
    "@types/node": "^12.12.5",
    "@types/papaparse": "^5.0.6",
    "@types/react": "^16.9.35",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-env": "^1.15.2",
    "@types/wordpress__block-editor": "^2.2.8",
    "@types/wordpress__components": "^9.0.0",
    "@types/wordpress__compose": "^3.4.0",
    "@types/wordpress__data": "^4.6.5",
    "@types/wordpress__edit-post": "^3.5.1",
    "@types/wordpress__editor": "^9.4.1",
    "@types/wordpress__hooks": "^2.4.0",
    "@types/wordpress__shortcode": "^2.3.0",
    "classnames": "^2.2.6"
  }
}
